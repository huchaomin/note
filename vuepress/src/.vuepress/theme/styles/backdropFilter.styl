$wrapper
  background rgba(255, 255, 255, 0.65)
  backdrop-filter blur(16px) saturate(33%)
  -webkit-backdrop-filter: blur(16px) saturate(33%)

.v-footer, .v-app-bar, .v-navigation-drawer, .breadcrumbs, .to-top
  background-color rgba(255, 255, 255, 0.95) !important
  backdrop-filter blur(16px) saturate(33%)
  -webkit-backdrop-filter: blur(16px) saturate(33%)

.v-navigation-drawer
  .v-navigation-drawer__border
    background-color transparent !important

.v-application {
  background-color: rgb(80, 227, 194) !important;
  background-image: radial-gradient(at 47% 33%, rgb(23, 181, 133) 0px, transparent 59%), radial-gradient(at 82% 65%, rgb(0, 179, 255) 0px, transparent 55%) !important;
}
